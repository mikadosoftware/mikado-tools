#! -*- coding:utf-8 -*-

from setuptools import setup, find_packages

# get version data
with open("VERSION") as fo:
    version = fo.read()

setup(
     name='mikado.tools',
     version=version,
     description='A Description to change',
     author='author',
     packages=find_packages(exclude=('tests')),
     scripts=['bin/hello',]
)
